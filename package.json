{
	"name": "project-one-front",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "concurrently \"vite --open\" \"yarn typed-scss-modules ./src\"",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint ./src --fix",
		"test": "vitest --config ./vitest.config.ts",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,html,css,scss,json}": [
			"yarn prettier --write"
		]
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.2.5",
		"antd": "^5.18.0",
		"classnames": "^2.5.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.23.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@feature-sliced/eslint-config": "feature-sliced/eslint-config",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^15.0.7",
		"@types/node": "^20.12.5",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitejs/plugin-react": "^4.2.1",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-boundaries": "^4.2.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"eslint-plugin-storybook": "^0.8.0",
		"husky": "^8.0.0",
		"jsdom": "^24.0.0",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prettier-eslint": "^16.3.0",
		"sass": "^1.74.1",
		"stylelint": "^16.3.1",
		"stylelint-config-clean-order": "^5.4.2",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-standard-scss": "^13.1.0",
		"typed-scss-modules": "^8.0.1",
		"typescript": "^5.2.2",
		"vite": "^5.2.0",
		"vite-plugin-svg-icons": "^2.0.1",
		"vitest": "^1.6.0"
	}
}
